package com.services.utility;   
import java.util.*;  
      
    public class GenerateRandomNumber 
    {  
     public static void main(String [] args){  
    	 
    	 Random r = new Random();
    	 int randomNo = r.nextInt((10-1)+1)+1;
    	 System.out.println(randomNo);
          
     }
    }  