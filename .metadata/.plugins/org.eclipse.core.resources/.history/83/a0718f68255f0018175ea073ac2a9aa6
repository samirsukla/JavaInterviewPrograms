package com.services.utility;   
import java.util.*;  
      
    public class GenerateRandomNumber 
    {  
     public int generateRandomno(){  
    	 
    	 Random r = new Random();
    	 int randomNo = r.nextInt((12-1)+1)+1;
    	 System.out.println(randomNo);
    	 return randomNo;
          
     }
    }  